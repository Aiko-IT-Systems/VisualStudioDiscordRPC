<UserControl 
	x:Class="VisualStudioDiscordRPC.Shared.CustomTextSlotEditor"
    xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
    xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
    xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
    xmlns:local="clr-namespace:VisualStudioDiscordRPC.Shared.ViewModels.CustomSlots"
    mc:Ignorable="d"
    d:Background="White"
    d:DataContext="{d:DesignInstance Type=local:CustomTextSlotViewModel}">
    <StackPanel>
        <TextBlock Text="Custom text:"/>
        <TextBox Text="{Binding CustomText}"/>
    </StackPanel>    
</UserControl>